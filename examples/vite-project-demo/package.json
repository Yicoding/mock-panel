{
  "name": "vite-project-demo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --mode development",
    "mock": "vite --mode mock",
    "start-test": "vite --mode test",
    "build-dev": "tsc && vite build --mode development",
    "build-test": "tsc && vite build --mode test",
    "build-uat": "tsc && vite build --mode uat",
    "build-prod": "tsc && vite build --mode production",
    "build-analyze": "VITE_BUILD_ANALYZER=true yarn build-prod",
    "preview": "vite preview",
    "lint": "eslint --fix --ext .ts,.tsx,.js,.jsx --quiet ./",
    "commit": "git-cz",
    "changelog": "standard-version -n",
    "prepare": "husky install"
  },
  "dependencies": {
    "@sentry/react": "^8.4.0",
    "@xmly/xmrep": "^2.4.24",
    "axios": "^1.7.2",
    "clsx": "^2.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "antd": "^5.17.3",
    "@ant-design/icons": "^5.3.7",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "@vitejs/plugin-legacy": "^5.4.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "autoprefixer": "^10.4.19",
    "commitizen": "^4.3.0",
    "commitlint": "^19.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.0.11",
    "less": "^4.2.0",
    "lint-staged": "^15.2.4",
    "mockjs": "^1.1.0",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "standard-version": "^9.5.0",
    "terser": "^5.31.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-mock": "^3.0.2",
    "vite-plugin-webpackchunkname": "^1.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts}": [
      "eslint --fix"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "yxpm": {
    "output": "dist"
  }
}